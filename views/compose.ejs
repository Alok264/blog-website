<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="input" class="label-text">Title</label>
        <input type="text" name="msg" id="input" class="form-control" autocomplete="off">
        <label for="textarea" class="label-text">Post</label>
        <textarea name="detail_msg" id="textarea" cols="100" class="form-control" rows="5"></textarea>
    </div>
    <button type="submit" class="btn btn-primary btn-md">Publish</button>
</form>
<%- include('partials/footer'); -%>